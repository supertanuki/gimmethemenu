{% extends '::base.html.twig' %}

{% block head_meta %}
    {% set meta = {
    'title': page.seoTitle|default(page.title),
    'description': page.seoDescription|default(page.body|striptags)
    } %}
    {% include 'ApplicationMainBundle:Common:headMeta.html.twig' with { 'meta': meta } %}
{% endblock %}

{% block body %}
    <h1>{{ page.title }}</h1>
    <div>
        {{ page.body|raw }}
    </div>
{% endblock %}
